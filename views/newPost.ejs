<%- include('./partials/header.ejs', { user: user }) %>

<body>
    <div class="mainWrapper">
        <form method = "POST" action="/newContent">
            <label for="photoTitle">Photo Title</label>
            <input type="text" name = "photoTitle">

            <label for="photoLocation">Photo Location</label>
            <input type="text" name = "photoLocation">

            <label for="camera">Camera Used</label>
            <input type="text" name = "camera">

